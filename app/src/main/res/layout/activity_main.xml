<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/Table"
        android:layout_width="313dp"
        android:layout_height="361dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.412"
        app:srcCompat="@drawable/table" />

    <ImageView
        android:id="@+id/field4"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:clickable="true"
        android:onClick="pos4"
        app:layout_constraintBottom_toTopOf="@+id/field7"
        app:layout_constraintEnd_toStartOf="@+id/field2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/field1"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field5"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:clickable="true"
        android:onClick="pos5"
        app:layout_constraintBottom_toTopOf="@+id/field8"
        app:layout_constraintEnd_toStartOf="@+id/field6"
        app:layout_constraintTop_toBottomOf="@+id/field2"
        app:layout_constraintVertical_bias="0.022"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field8"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_marginBottom="136dp"
        android:clickable="true"
        android:onClick="pos8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/field9"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toEndOf="@+id/field7"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field6"
        android:layout_width="67dp"
        android:layout_height="60dp"
        android:layout_marginEnd="23dp"
        android:layout_marginRight="23dp"
        android:clickable="true"
        android:onClick="pos6"
        app:layout_constraintBottom_toTopOf="@+id/field9"
        app:layout_constraintEnd_toEndOf="@+id/Table"
        app:layout_constraintTop_toBottomOf="@+id/field3"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field9"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="132dp"
        android:clickable="true"
        android:onClick="pos9"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field7"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="136dp"
        android:clickable="true"
        android:onClick="pos7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field1"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="108dp"
        android:clickable="true"
        android:onClick="pos1"
        app:layout_constraintEnd_toStartOf="@+id/field2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field2"
        android:layout_width="64dp"
        android:layout_height="57dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="128dp"
        android:layout_marginRight="128dp"
        android:clickable="true"
        android:onClick="pos2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/transparent" />

    <ImageView
        android:id="@+id/field3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:clickable="true"
        android:onClick="pos3"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/transparent" />

    <TextView
        android:id="@+id/Result"
        android:layout_width="252dp"
        android:layout_height="46dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>